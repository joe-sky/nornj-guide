{
  "gitbook": "3.2.2",
  "plugins": [
    "anchors",
    "jsfiddle",
    "edit-link",
    "github",
    "prism",
    "advanced-emoji",
    "i18n-revised",
    "-sharing",
    "back-to-top-button",
    "theme-default",
    "-lunr",
    "-search",
    "search-plus",
    "expandable-chapters-revised",
    "anchor-navigation-ex",
    "versions-select"
  ],
  "pluginsConfig": {
    "jsfiddle": {
      "type": "frame",
      "tabs": ["result", "html", "js", "css"],
      "height": "200",
      "width": "500",
      "fontColor": "00FF00"
    },
    "edit-link": {
      "base": "https://github.com/joe-sky/nornj-guide/blob/master",
      "label": "Edit this page"
    },
    "github": {
      "url": "https://github.com/joe-sky/nornj"
    },
    "prism": {
      "css": ["prismjs/themes/prism-tomorrow.css"]
    },
    "i18n": {
      "all": ["english", "chinese"],
      "ChineseUrl": "/zh/",
      "EnglishUrl": "/en/"
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "showGoTop": false
    },
    "versions": {
      "options": [
        {
          "value": "https://joe-sky.gitbooks.io/nornj-guide/content/zh/",
          "text": "v5.0.0"
        },
        {
          "value": "https://joe-sky.github.io/nornj-guide/",
          "text": "v0.4.18"
        }
      ]
    }
  }
}
